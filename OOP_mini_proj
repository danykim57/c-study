#include<iostream>
#include<string>
#include<vector>
#include<time.h>
using namespace std;

class Organism {
public:
	Organism() : moves(0), isMoved(false) {};
	virtual ~Organism() = default;
	virtual char getType() const = 0;
	virtual void breed() = 0;
	virtual void move() = 0;
	virtual void die() {};
	int getMoves() { return moves; }
	void setMoves(int newMoves) { this->moves = newMoves; }
	void addMoves() { moves++; }
	bool isMoved = false;
	
	Organism(const Organism& a) { isMoved = a.isMoved; moves = a.moves; }
	Organism& operator =(const Organism& a) {
		if (&a != this) {
			isMoved = a.isMoved;
			moves = a.moves;
		}
		return *this;
	}

protected:
	int moves;
};

//Global init of 2D vector
vector<vector<Organism*>>world(20, vector<Organism*>(20));

void deletePointed(Organism* ptr) {
	delete ptr;
}


class Ant : public Organism {
	char type = 'o';
	int x = 0 , y = 0;
	int moves = 0;
public:
	Ant() : Organism() {
		bool isEmpty = false;
		while (isEmpty == false) {
			x = rand() % 20;
			y = rand() % 20;
			if (world[x][y] == NULL) {
				isEmpty = true;
			}
		}
		world[x][y] = this;
		
	};
	virtual ~Ant() = default;

	Ant(int x, int y) : Organism() {
		this->x = x;
		this->y = y;
		world[x][y] = this;

	};

	Ant(const Ant& a) { isMoved = a.isMoved; moves = a.moves; type = a.type; x = a.x; y = a.y; }
	Ant operator =(const Ant& a) {
		if (&a != this) {
			isMoved = a.isMoved;
			moves = a.moves;
			type = a.type;
			x = a.x; 
			y = a.y;
		}
		return *this;
	}
	virtual void move();
	virtual char getType() const { return type; }
	virtual void breed();
	virtual void die() {};
};

void Ant::move() { 
	int num = rand() % 4;
	//left
	if (num == 0) {
		int n = x - 1;
		if (x > 0 && world[n][y] == NULL) {
			world[x][y] = NULL;
			x--;
			world[x][y] = this;

		}
	}
	//right
	if (num == 1) {
		int n = x + 1;
		if (x < 19 && world[n][y] == NULL) {
			world[x][y] = NULL;
			x++;
			world[x][y] = this;
		}
	}
	//up
	if (num == 2) {
		int n = y - 1;
		if (y > 0 && world[x][n] == NULL) {
			world[x][y] = NULL;
			y--;
			world[x][y] = this;
		}
	}
	//down
	if (num == 3) {
		int n = y + 1;
		if (y < 19 && world[x][n] == NULL) {
			world[x][y] = NULL;
			y++;
			world[x][y] = this;
		}
	}
	this->isMoved = true;
	this->addMoves();
	this->breed();
};


void Ant::breed() {
	if (this->getMoves() == 3) {
		int i = this->x, j = this->y;
		if ( i < 19 && world[++i][j] == NULL) {
			world[i][j] = new Ant(i, j);
			int i = this->x, j = this->y;
		}
		else if ( i > 0 && world[--i][j] == NULL) {
			world[i][j] = new Ant(i, j);
			int i = this->x, j = this->y;
		}
		else if ( j < 19 && world[i][++j] == NULL) {
			world[i][j] = new Ant(i, j);
			int i = this->x, j = this->y;
		}
		else if ( j > 0 && world[i][--j] == NULL) {
			world[i][j] = new Ant(i, j);
			int i = this->x, j = this->y;
		}
		this->setMoves(0);
	}
}

class Doodlebug : public Organism {
	char type = 'X';
	int x = 0, y = 0;
	int moves = 0;
	int starve = 0;
public:
	Doodlebug() : Organism() {
		bool isEmpty = false;
		while (isEmpty == false) {
			x = rand() % 20;
			y = rand() % 20;
			if (world[x][y] == NULL) {
				isEmpty = true;
			}
		}
		world[x][y] = this;
	};
	virtual ~Doodlebug() = default;

	Doodlebug(int x, int y) : Organism(), starve(0) {
		this->setX(x);
		this->setY(y);
		world[x][y] = this;
	};

	Doodlebug(const Doodlebug& a) { isMoved = a.isMoved; moves = a.moves; type = a.type; x = a.x; y = a.y; starve = a.starve; }
	Doodlebug operator =(const Doodlebug& a) {
		if (&a != this) {
			isMoved = a.isMoved;
			moves = a.moves;
			type = a.type;
			x = a.x;
			y = a.y;
			starve = a.starve;
		}
		return *this;
	}

	virtual void move();
	virtual char getType() const { return type; }
	virtual void breed();
	virtual void die();
	int getX() { return this->x; }
	void setX(int newX) { this->x = newX; }
	int getY() { return this->y; }
	void setY(int newY) { this->y = newY; }

};

void Doodlebug::move() {
	bool isAntAround = false;
	int n = x - 1;
	if (isAntAround == false && x > 0) {
		//left
		if (world[n][y] != NULL && world[n][y]->getType() == 'o') {
			world[x][y] = NULL;
			x--;
			delete world[x][y];
			world[x][y] = this;
			this->starve = 0;
			isAntAround = true;
		}
	}
	n = x + 1;
	if (isAntAround == false && x < 19) {
		if (world[n][y] != NULL && world[n][y]->getType() == 'o') {
			world[x][y] = NULL;
			x++;
			delete world[x][y];
			world[x][y] = this;
			this->starve = 0;
			isAntAround = true;
		}
	}
	n = y - 1;
	if (isAntAround == false && y > 0) {
		//up
		if (world[x][n] != NULL && world[x][n]->getType() == 'o') {
			world[x][y] = NULL;
			y--;
			delete world[x][y];
			world[x][y] = this;
			this->starve = 0;
			isAntAround = true;
		}
	}
	n = y + 1;
	if (isAntAround == false && y < 19) {
		//down
		if (world[x][n] != NULL && world[x][n]->getType() == 'o') {
			world[x][y] = NULL;
			y++;
			delete world[x][y];
			world[x][y] = this;
			this->starve = 0;
			isAntAround = true;
		}
	} 

	if (isAntAround == false) {
		int num = rand() % 4;
		//left
		if (num == 0) {
			int n = x - 1;
			if (x > 0 && world[n][y] == NULL) {
				world[x][y] = NULL;
				x--;
				world[x][y] = this;
				this->starve++;
			}
		}
		//right
		if (num == 1) {
			int n = x + 1;
			if (x < 19 && world[n][y] == NULL) {
				world[x][y] = NULL;
				x++;
				world[x][y] = this;
				this->starve++;
			}
		}
		//up
		if (num == 2) {
			int n = y - 1;
			if (y > 0 && world[x][n] == NULL) {
				world[x][y] = NULL;
				y--;
				world[x][y] = this;
				this->starve++;
			}
		}
		//down
		if (num == 3) {
			int n = y + 1;
			if (y < 19 && world[x][n] == NULL) {
				world[x][y] = NULL;
				y++;
				world[x][y] = this;
				this->starve++;
			}
		}

	}
	this->isMoved = true;
	this->addMoves();
	this->breed();
	this->die();
};

void Doodlebug::breed() {
	if (this->getMoves() == 8) {
		int i = this->getX(), j = this->getY();
		if (i < 19 && world[++i][j] == NULL) {
			world[i][j] = new Doodlebug(i, j);
			int i = this->getX(), j = this->getY();
		}
		else if (i > 0 && world[--i][j] == NULL) {
			world[i][j] = new Doodlebug(i, j);
			int i = this->getX(), j = this->getY();
		}
		else if (j < 19 && world[i][++j] == NULL) {
			world[i][j] = new Doodlebug(i, j);
			int i = this->getX(), j = this->getY();
		}
		else if (j > 0 && world[i][--j] == NULL) {
			world[i][j] = new Doodlebug(i, j);
			int i = this->getX(), j = this->getY();
			
		}
		this->setMoves(0);
	}
}

void Doodlebug::die() {
	if (this->starve == 3) {
		int x = this->getX();
		int y = this->getY();
		delete world[x][y];
		world[x][y] = NULL;
	}
}

void createWorld(vector<vector<Organism*>>& world) {
	srand(time(NULL));
	for (int x = 0; x < world.size(); x++) {
		for (int y = 0; y < 20; y++) {
			world[x][y] = NULL;
		}
	}
	
	for (int i = 0; i < 100; i++) {
		Ant* ant = new Ant();
	}
	
	
	for (int i = 0; i < 5; i++) {
		Doodlebug* doodlebug = new Doodlebug();
	}
}

void printWorld(vector<vector<Organism*>>& world) {

	for (int x = 0; x < world.size(); x++) {
		for (int y = 0; y < 20; y++) {
			if (world[x][y] == NULL) {
				cout << '-' << ' ';
			}
			else {
				cout << world[x][y]->getType() << ' ';
			}
		}
		cout << endl;
	}

}

void changeWorld(vector<vector<Organism*>>& world) {
	for (int x = 0; x < world.size(); x++) {
		for (int y = 0; y < 20; y++) {
			if (world[x][y] != NULL)
			{
				world[x][y]->isMoved = false;
			}
		}
	}

	
	for (int x = 0; x < world.size(); x++) {
		for (int y = 0; y < 20; y++) {
			if (world[x][y] != NULL && world[x][y]->getType() == 'X' && world[x][y]->isMoved == false)
			{
				world[x][y]->move();
			}
		}
	}

	for (int x = 0; x < world.size(); x++) {
		for (int y = 0; y < 20; y++) {
			if (world[x][y] != NULL && world[x][y]->getType() == 'o' && world[x][y]->isMoved == false)
			{
				world[x][y]->move();
			}
		}
	}
}



int main() {
	createWorld(world);
	printWorld(world);
	
	while (cin.get() == '\n') {
		changeWorld(world);
		printWorld(world);
		
	}

	return 0;
}
